    "option": {
        "账号属性": {
            "type": "select",
            "default_case": "安卓QQ",
            "cases": [
                {
                    "name": "安卓QQ",
                    "pipeline_override": {
                        "start_naruto": {
                            "package": "com.tencent.KiHan"
                        },
                        "shut_qq": {
                            "package": "com.tencent.mobileqq"
                        },
                        "shut_wechat": {
                            "package": "com.tencent.mm"
                        }
                    }
                },
                {
                    "name": "苹果",
                    "pipeline_override": {
                        "close_ninjia_book_wechat_share": {
                            "package": "com.tencent.xin"
                        },
                        "close_wechat_app": {
                            "package": "com.tencent.xin"
                        }
                    }
                }
            ]
        },
        "不做修行": {
            "type": "select",
            "default_case": "做修行",
            "cases": [
                {
                    "name": "做修行",
                    "pipeline_override": {
                        "practice_place_undone": {
                            "next": [
                                "train_road"
                            ]
                        }
                    }
                },
                {
                    "name": "不做修行",
                    "pipeline_override": {
                        "practice_place_undone": {
                            "next": [
                                "survival_challenge"
                            ]
                        },
                        "practice_place_in_center_enter": {
                            "next": [
                                "survival_challenge_ac_entry_undone",
                                "survival_challenge_ac_done"
                            ]
                        },
                        "practice_place_ac_entry": {
                            "next": [
                                "survival_challenge_ac_entry_undone",
                                "survival_challenge_ac_done"
                            ]
                        }
                    }
                }
            ]
        },
        "匹配还是排位": {
            "type": "switch",
            "default_case": "匹配",
            "cases": [
                {
                    "name": "匹配",
                    "pipeline_override": {
                        "weekly_win_in_duel_field": {
                            "next": [
                                "weekly_win_match"
                            ]
                        },
                        "weekly_win_entry": {
                            "next": [
                                "weekly_win_match"
                            ]
                        }
                    }
                },
                {
                    "name": "排位(beta)",
                    "pipeline_override": {
                        "weekly_win_in_duel_field": {
                            "next": [
                                "weekly_win_rank"
                            ]
                        }
                    }
                }
            ]
        },
        "战斗时是否点击普攻": {
            "type": "select",
            "cases": [
                {
                    "name": "否",
                    "pipeline_override": {
                        "hundred_ninjia_check_fight": {
                            "action": "DoNothing"
                        }
                    }
                },
                {
                    "name": "是",
                    "pipeline_override": {
                        "hundred_ninjia_check_fight": {
                            "action": "Click"
                        }
                    }
                }
            ]
        },
        "是否领取体验卡宝箱": {
            "type": "select",
            "default_case": "否",
            "cases": [
                {
                    "name": "是",
                    "pipeline_override": {
                        "get_ninjia_book_award": {
                            "template": [
                                "Ninjia_book/seal_roll_waiting.png",
                                "Ninjia_book/chopper_60_waiting.png",
                                "Ninjia_book/fame_waiting.png",
                                "Ninjia_book/gold_waiting.png",
                                "Ninjia_book/star_waiting.png",
                                "Ninjia_book/energe_waiting.png",
                                "Ninjia_book/soul_waiting.png",
                                "Ninjia_book/box_waiting.png",
                                "Ninjia_book/wooden_spike_roll_waiting.png",
                                "Ninjia_book/title_waiting.png"
                            ]
                        }
                    }
                },
                {
                    "name": "否",
                    "pipeline_override": {
                        "get_ninjia_book_award": {
                            "template": [
                                "Ninjia_book/seal_roll_waiting.png",
                                "Ninjia_book/chopper_60_waiting.png",
                                "Ninjia_book/fame_waiting.png",
                                "Ninjia_book/gold_waiting.png",
                                "Ninjia_book/star_waiting.png",
                                "Ninjia_book/energe_waiting.png",
                                "Ninjia_book/soul_waiting.png",
                                "Ninjia_book/wooden_spike_roll_waiting.png",
                                "Ninjia_book/title_waiting.png"
                            ]
                        }
                    }
                }
            ]
        },
        "是否领取奖励": {
            "type": "select",
            "default_case": "否",
            "cases": [
                {
                    "name": "是",
                    "pipeline_override": {
                        "ninjia_book_award_undone": {
                            "next": [
                                "ninjia_book_award_undone",
                                "ninjia_book_award_swip_to_left"
                            ]
                        }
                    }
                },
                {
                    "name": "否",
                    "pipeline_override": {
                        "ninjia_book_award_undone": {
                            "next": [
                                "back_main_screen_and_stop"
                            ]
                        }
                    }
                }
            ]
        },
        "选择小游戏": {
            "cases": [
                {
                    "name": "聚宝试炼领取奖励",
                    "pipeline_override": {
                        "mini_game": {
                            "next": "treasure_gathering_trial_award"
                        }
                    }
                }
            ]
        },
        "胜利一次就退出": {
            "type": "select",
            "cases": [
                {
                    "name": "No",
                    "pipeline_override": {}
                },
                {
                    "name": "Yes",
                    "pipeline_override": {
                        "weekly_win_success": {
                            "next": "back_main_screen_and_stop"
                        }
                    }
                }
            ]
        },
        "借组织助战": {
            "cases": [
                {
                    "name": "开,借人",
                    "pipeline_override": {
                        "team_dash_invite_page": {
                            "next": [
                                "team_dash_invite_ninjia"
                            ]
                        }
                    }
                },
                {
                    "name": "关,不借人",
                    "pipeline_override": {
                        "team_dash_invite_page": {
                            "next": [
                                "dash_or_exit"
                            ]
                        }
                    }
                }
            ]
        },
        "积分赛战斗选项": {
            "cases": [
                {
                    "name": "开启战斗",
                    "pipeline_override": {
                        "point_race": {
                            "next": [
                                "point_race_check_in_main_screen"
                            ],
                            "interrupt": [
                                "back_main_screen"
                            ]
                        }
                    }
                },
                {
                    "name": "关闭战斗",
                    "pipeline_override": {
                        "point_race": {
                            "next": [
                                "point_race_in_center_enter"
                            ]
                        },
                        "point_race_ac_entry_undone": {
                            "next": [
                                "back_main_screen_and_stop"
                            ]
                        }
                    }
                }
            ]
        }
