{
    "name": "MaaAutoNaruto",
    "version": "v0.2.5",
    "mirrorchyan_rid": "MaaAutoNaruto",
    "mirrorchyan_multiplatform": false,
    "url": "https://github.com/duorua/narutomobile",
    "mfa_max_version": "v1.8.9",
    "mfa_min_version": "v1.8.9",
    "agent": {
        "child_exec": "python",
        "child_args": [
            "-u",
            "{PROJECT_DIR}/../agent/main.py"
        ]
    },
    "controller": [
        {
            "name": "连接模拟器",
            "type": "Adb"
        }
    ],
    "resource": [
        {
            "name": "官服",
            "path": [
                "{PROJECT_DIR}/resource/base"
            ]
        }
    ],
    "task": [
        {
            "name": "进入火影",
            "entry": "start_up",
            "option": [
                "账号属性"
            ]
        },
        {
            "name": "轻松助手",
            "entry": "easy_helper"
        },
        {
            "name": "招财",
            "entry": "get_chopper"
        },
        {
            "name": "排行榜",
            "entry": "leaderboard"
        },
        {
            "name": "送体力",
            "entry": "give_energy"
        },
        {
            "name": "每日分享",
            "entry": "share"
        },
        {
            "name": "组织",
            "entry": "group"
        },
        {
            "name": "领取积分赛奖励",
            "entry": "point_race",
            "doc": "[size:15][color:tomato]该功能不会进行战斗，仅领取奖励[/color][/size]"
        },
        {
            "name": "试炼",
            "entry": "practice_place",
            "option": [
                "不做修行",
                "等待生存扫荡时间选择"
            ]
        },
        {
            "name": "小队突袭",
            "entry": "team_dash"
        },
        {
            "name": "免费招募",
            "entry": "headhunt"
        },
        {
            "name": "月签到和活动",
            "entry": "activity"
        },
        {
            "name": "任务集会所(beta)",
            "entry": "mission_office"
        },
        {
            "name": "刷体力(beta)",
            "entry": "use_energy"
        },
        {
            "name": "丰饶之间",
            "entry": "rich_room"
        },
        {
            "name": "忍法帖",
            "entry": "ninjia_book",
            "doc": "[size:15]开启后会领取忍法帖升级的[color:red]全部奖励[/color]，需要攒体验卡的不要用！[/size]",
            "option": [
                "是否领取体验卡宝箱",
                "是否领取奖励"
            ]
        },
        {
            "name": "火影情报社",
            "entry": "naruto_club"
        },
        {
            "name": "邮箱",
            "entry": "mail"
        },
        {
            "name": "活跃宝箱",
            "entry": "liveness_award"
        },
        {
            "name": "周胜(没做完)",
            "entry": "weekly_win",
            "option": [
                "匹配还是排位"
            ]
        },
        {
            "name": "冒险副本(beta)",
            "entry": "advanture"
        },
        {
            "name": "精英副本(beta)",
            "entry": "elite_instance"
        },
        {
            "name": "修罗副本(beta)",
            "entry": "asura_instance"
        },
        {
            "name": "百忍大战",
            "entry": "hundred_ninjia",
            "option": [
                "战斗时是否点击普攻"
            ]
        },
        {
            "name": "agent Debug",
            "entry": "use_agent"
        },
        {
            "name": "小游戏",
            "entry": "mini_game",
            "option": [
                "选择小游戏"
            ]
        }
    ],
    "option": {
        "账号属性": {
            "cases": [
                {
                    "name": "安卓QQ",
                    "pipeline_override": {
                        "start_naruto": {
                            "package": "com.tencent.KiHan"
                        },
                        "shut_qq": {
                            "package": "com.tencent.mobileqq"
                        },
                        "shut_wechat": {
                            "package": "com.tencent.mm"
                        }
                    }
                },
                {
                    "name": "苹果",
                    "pipeline_override": {
                        "close_ninjia_book_wechat_share": {
                            "package": "com.tencent.xin"
                        },
                        "close_wechat_app": {
                            "package": "com.tencent.xin"
                        }
                    }
                }
            ]
        },
        "不做修行": {
            "cases": [
                {
                    "name": "做修行",
                    "pipeline_override": {
                        "practice_place_undone": {
                            "next": [
                                "train_road"
                            ]
                        }
                    }
                },
                {
                    "name": "不做修行",
                    "pipeline_override": {
                        "practice_place_undone": {
                            "next": [
                                "survival_challenge"
                            ]
                        },
                        "practice_place_in_center_enter": {
                            "next": [
                                "survival_challenge_ac_entry_undone",
                                "survival_challenge_ac_done"
                            ]
                        },
                        "practice_place_ac_entry": {
                            "next": [
                                "survival_challenge_ac_entry_undone",
                                "survival_challenge_ac_done"
                            ]
                        }
                    }
                }
            ]
        },
        "等待生存扫荡时间选择": {
            "cases": [
                {
                    "name": "等待20秒",
                    "pipeline_override": {
                        "survival_challenge_sweep_ticket_confirm": {
                            "post_delay": 20000
                        }
                    }
                },
                {
                    "name": "等待30秒",
                    "pipeline_override": {
                        "survival_challenge_sweep_ticket_confirm": {
                            "post_delay": 30000
                        }
                    }
                },
                {
                    "name": "等待40秒",
                    "pipeline_override": {
                        "survival_challenge_sweep_ticket_confirm": {
                            "post_delay": 40000
                        }
                    }
                }
            ]
        },
        "匹配还是排位": {
            "cases": [
                {
                    "name": "匹配",
                    "pipeline_override": {
                        "weekly_win_in_duel_field": {
                            "next": [
                                "weekly_win_match"
                            ]
                        },
                        "weekly_win_entry": {
                            "next": [
                                "weekly_win_match"
                            ]
                        }
                    }
                },
                {
                    "name": "排位(没做完)",
                    "pipeline_override": {
                        "weekly_win_in_duel_field": {
                            "next": [
                                "weekly_win_rank"
                            ]
                        },
                        "weekly_win_entry": {
                            "next": [
                                "weekly_win_rank"
                            ]
                        }
                    }
                }
            ]
        },
        "战斗时是否点击普攻": {
            "cases": [
                {
                    "name": "否",
                    "pipeline_override": {
                        "hundred_ninjia_check_fight": {
                            "action": "DoNothing"
                        }
                    }
                },
                {
                    "name": "是",
                    "pipeline_override": {
                        "hundred_ninjia_check_fight": {
                            "action": "Click"
                        }
                    }
                }
            ]
        },
        "是否领取体验卡宝箱": {
            "cases": [
                {
                    "name": "是",
                    "pipeline_override": {
                        "get_ninjia_book_award": {
                            "template": [
                                "Ninjia_book/seal_roll_waiting.png",
                                "Ninjia_book/chopper_60_waiting.png",
                                "Ninjia_book/fame_waiting.png",
                                "Ninjia_book/gold_waiting.png",
                                "Ninjia_book/star_waiting.png",
                                "Ninjia_book/energe_waiting.png",
                                "Ninjia_book/soul_waiting.png",
                                "Ninjia_book/box_waiting.png",
                                "Ninjia_book/wooden_spike_roll_waiting.png",
                                "Ninjia_book/title_waiting.png"
                            ]
                        }
                    }
                },
                {
                    "name": "否",
                    "pipeline_override": {
                        "get_ninjia_book_award": {
                            "template": [
                                "Ninjia_book/seal_roll_waiting.png",
                                "Ninjia_book/chopper_60_waiting.png",
                                "Ninjia_book/fame_waiting.png",
                                "Ninjia_book/gold_waiting.png",
                                "Ninjia_book/star_waiting.png",
                                "Ninjia_book/energe_waiting.png",
                                "Ninjia_book/soul_waiting.png",
                                "Ninjia_book/wooden_spike_roll_waiting.png",
                                "Ninjia_book/title_waiting.png"
                            ]
                        }
                    }
                }
            ]
        },
        "是否领取奖励": {
            "cases": [
                {
                    "name": "是",
                    "pipeline_override": {
                        "ninjia_book_award_undone": {
                            "next": [
                                "ninjia_book_award_undone",
                                "ninjia_book_award_swip_to_left"
                            ]
                        }
                    }
                },
                {
                    "name": "否",
                    "pipeline_override": {
                        "ninjia_book_award_undone": {
                            "next": [
                                "back_main_screen_and_stop"
                            ],
                            "interrupt": [
                                "stoptask"
                            ]
                        }
                    }
                }
            ]
        },
        "选择小游戏": {
            "cases": [
                {
                    "name": "聚宝试炼领取奖励",
                    "pipeline_override": {
                        "mini_game": {
                            "next": "treasure_gathering_trial_award"
                        }
                    }
                }
            ]
        }
    }
}