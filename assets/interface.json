{
    "name": "MaaAutoNaruto",
    "version": "v0.2.4-beta.2",
    "mirrorchyan_rid": "MaaAutoNaruto",
    "mirrorchyan_multiplatform": false,
    "url": "https://github.com/duorua/narutomobile",
    "message": "你现在使用的是一个测试版本，她并不稳定",
    "agent": {
        "child_exec": "python",
        "child_args": [
            "-u",
            "{PROJECT_DIR}/../agent/main.py"
        ]
    },
    "controller": [
        {
            "name": "安卓端",
            "type": "Adb"
        }
    ],
    "resource": [
        {
            "name": "官服",
            "path": [
                "{PROJECT_DIR}/resource/base"
            ]
        }
    ],
    "task": [
        {
            "name": "进入火影",
            "entry": "start_up",
            "check": true,
            "option": [
                "账号属性"
            ]
        },
        {
            "name": "轻松助手",
            "entry": "easy_helper",
            "check": true
        },
        {
            "name": "招财",
            "entry": "get_chopper",
            "check": true
        },
        {
            "name": "排行榜",
            "entry": "leaderboard",
            "check": true
        },
        {
            "name": "送体力",
            "entry": "give_energy",
            "check": true
        },
        {
            "name": "每日分享",
            "entry": "share",
            "check": true
        },
        {
            "name": "组织",
            "entry": "group",
            "check": true
        },
        {
            "name": "邮箱",
            "entry": "mail",
            "check": true
        },
        {
            "name": "积分赛(只会领奖励)",
            "entry": "point_race",
            "check": true
        },
        {
            "name": "试炼",
            "entry": "practice_place",
            "option": [
                "不做修行",
                "等待生存扫荡时间选择"
            ],
            "check": true
        },
        {
            "name": "小队突袭",
            "entry": "team_dash",
            "check": true
        },
        {
            "name": "免费招募",
            "entry": "headhunt",
            "check": true
        },
        {
            "name": "月签到和活动",
            "entry": "activity",
            "check": true
        },
        {
            "name": "任务集会所(beta)",
            "entry": "mission_office",
            "check": true
        },
        {
            "name": "刷体力(beta)",
            "entry": "use_energy",
            "check": true
        },
        {
            "name": "丰饶",
            "entry": "rich_room",
            "check": true
        },
        {
            "name": "忍法帖",
            "entry": "ninjia_book",
            "check": true
        },
        {
            "name": "火影情报社",
            "entry": "naruto_club",
            "check": true
        },
        {
            "name": "活跃宝箱(beta)",
            "entry": "liveness_award",
            "check": true
        },
        {
            "name": "周胜(只能刷人机)",
            "entry": "weekly_win",
            "option": [
                "匹配还是排位"
            ]
        },
        {
            "name": "百忍大战",
            "entry": "hundred_ninjia",
            "option": [
                "战斗时是否点击普攻"
            ]
        },
        {
            "name": "agent Debug",
            "entry": "use_agent"
        }
    ],
    "option": {
        "账号属性": {
            "cases": [
                {
                    "name": "安卓QQ",
                    "pipeline_override": {
                        "start_naruto": {
                            "package": "com.tencent.KiHan"
                        },
                        "account_type": {
                            "next": "android_qq"
                        },
                        "naruto_club_entry": {
                            "next": "start_qq"
                        },
                        "shut_qq": {
                            "package": "com.tencent.mobileqq"
                        },
                        "shut_wechat": {
                            "package": "com.tencent.mm"
                        }
                    }
                },
                {
                    "name": "苹果",
                    "pipeline_override": {
                        "close_ninjia_book_wechat_share": {
                            "package": "com.tencent.xin"
                        },
                        "close_wechat_app": {
                            "package": "com.tencent.xin"
                        }
                    }
                }
            ]
        },
        "不做修行": {
            "cases": [
                {
                    "name": "做修行",
                    "pipeline_override": {
                        "practice_place_undone": {
                            "next": [
                                "train_road"
                            ]
                        }
                    }
                },
                {
                    "name": "不做修行",
                    "pipeline_override": {
                        "practice_place_undone": {
                            "next": [
                                "survival_challenge"
                            ]
                        },
                        "practice_place_in_center_enter": {
                            "next": [
                                "survival_challenge_ac_entry_undone",
                                "survival_challenge_ac_done"
                            ]
                        },
                        "practice_place_ac_entry": {
                            "next": [
                                "survival_challenge_ac_entry_undone",
                                "survival_challenge_ac_done"
                            ]
                        }
                    }
                }
            ]
        },
        "等待生存扫荡时间选择": {
            "cases": [
                {
                    "name": "等待20秒",
                    "pipeline_override": {
                        "survival_challenge_sweep_ticket_confirm": {
                            "post_delay": 20000
                        }
                    }
                },
                {
                    "name": "等待30秒",
                    "pipeline_override": {
                        "survival_challenge_sweep_ticket_confirm": {
                            "post_delay": 30000
                        }
                    }
                },
                {
                    "name": "等待40秒",
                    "pipeline_override": {
                        "survival_challenge_sweep_ticket_confirm": {
                            "post_delay": 40000
                        }
                    }
                }
            ]
        },
        "匹配还是排位": {
            "cases": [
                {
                    "name": "匹配",
                    "pipeline_override": {
                        "weekly_win_in_duel_field": {
                            "next": [
                                "weekly_win_match"
                            ]
                        },
                        "weekly_win_entry": {
                            "next": [
                                "weekly_win_match"
                            ]
                        }
                    }
                },
                {
                    "name": "排位(没做完)",
                    "pipeline_override": {
                        "weekly_win_in_duel_field": {
                            "next": [
                                "weekly_win_rank"
                            ]
                        },
                        "weekly_win_entry": {
                            "next": [
                                "weekly_win_rank"
                            ]
                        }
                    }
                }
            ]
        },
        "战斗时是否点击普攻": {
            "cases": [
                {
                    "name": "否",
                    "pipeline_override": {
                        "hundred_ninjia_check_fight": {
                            "action": "DoNothing"
                        }
                    }
                },
                {
                    "name": "是",
                    "pipeline_override": {
                        "hundred_ninjia_check_fight": {
                            "action": "Click"
                        }
                    }
                }
            ]
        }
    }
}